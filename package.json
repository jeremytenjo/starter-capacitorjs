{
  "name": "starter-capacitorjs",
  "description": "Capacitor JS Starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "node ./devtools/scripts/dev",
    "dev:app": "node ./devtools/webpack/scripts/startDevServer",
    "dev:storybook": "start-storybook -c ./devtools/storybook -p 6007 -s ./public --quiet --ci",
    "emulators:start": "firebase emulators:start --only auth,firestore",
    "build": "node ./devtools/webpack/scripts/buildApp",
    "android:run": "npm run android:sync && cap run android",
    "android:open": "npm run android:sync && cap open android",
    "android:sync": "npm run build && cap sync android",
    "ios:open": "npm run ios:sync && cap open ios",
    "ios:sync": "npm run build && cap sync ios",
    "capacitor:updateConfig": "npx cap copy",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "tests:run": "./devtools/scripts/tests/run",
    "serve": "node ./devtools/webpack/scripts/serveBuiltApp",
    "prettify": "prettier \"**/*.js\" --write --config ./devtools/prettier/config.js --ignore-path ./devtools/prettier/.prettierignore",
    "lint": "eslint ./ --fix",
    "git:hooks:setup": "git config core.hooksPath .githooks",
    "preinstall": "npm run git:hooks:setup"
  },
  "dependencies": {
    "@babel/core": "^7.12.17",
    "@babel/eslint-parser": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@capacitor/android": "^3.1.2",
    "@capacitor/cli": "^3.1.2",
    "@capacitor/core": "^3.1.2",
    "@capacitor/geolocation": "^0.4.0",
    "@capacitor/ios": "^3.1.2",
    "@capacitor/status-bar": "^0.4.3",
    "@capacitor/toast": "^0.4.3",
    "@check-light-or-dark/color": "^1.0.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@material-ui/core": "^5.0.0-alpha.30",
    "@material-ui/icons": "^4.11.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@storybook/addon-controls": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@useweb/lib": "^0.24.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-auto-import": "^1.1.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.2",
    "cypress": "^7.5.0",
    "dotenv-webpack": "^6.0.0",
    "error-overlay-webpack-plugin": "^0.4.2",
    "eslint": "^7.20.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.2",
    "filehound": "^1.17.4",
    "firebase": "^8.2.9",
    "framer-motion": "^4.1.11",
    "history": "^5.0.0",
    "html-webpack-plugin": "^5.2.0",
    "install": "^0.13.0",
    "joi": "^17.4.0",
    "live-server": "^1.2.1",
    "lodash": "^4.17.21",
    "my-local-ip": "^1.0.0",
    "node-emoji": "^1.10.0",
    "npm": "^7.6.3",
    "prettier": "^2.2.1",
    "prettier-plugin-java": "^1.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.0.0-rc.3",
    "react-query": "^3.12.3",
    "react-refresh": "^0.9.0",
    "react-router-dom": "^6.0.0-beta.0",
    "storybook": "^6.2.9",
    "style-loader": "^2.0.0",
    "webpack": "^5.23.0",
    "webpack-dev-server": "^3.11.2"
  }
}
